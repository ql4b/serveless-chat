{
  "name": "serverless-chat",
  "version": "0.0.1",
  "description": "Serveless Chat",
  "main": "src/hander.js",
  "scripts": {
    "lint": "eslint .",
    "lint:ci": "npm run lint -- --max-warnings 25",
    "lint:fix": "npm run lint -- --fix",
    "audit": "npm audit",
    "audit:fix": "npm audit --fix",
    "test": "jest --ci --colors",
    "test:ci": "npm test -- --coverage --silent --bail=5",
    "ci": "npm audit && npm run lint && npm run test:ci"
  },
  "pre-commit": [
    "audit:fix",
    "lint:fix",
    "test:ci"
  ],
  "author": "carlo@ql4b.net",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.1.0",
    "aws-sdk": "^2.818.0"
  }
}
